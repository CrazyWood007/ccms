    "ccms-antd": "^1.3.0",
    "ccms-antd-mini": "^1.3.0",
    "copy-html-to-clipboard": "^4.0.1",
    "file-loader": "^6.2.0",
    "immer": "^9.0.7",
    "lodash": "^4.17.21",
    "marked": "^2.0.1",
    "qiankun": "^2.7.0",
    "query-string": "^7.0.0",
    "react": "^17.0.1",
    "react-color": "^2.19.3",
    "react-dom": "^17.0.1",
